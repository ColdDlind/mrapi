{
  "name": "basic",
  "version": "0.0.1",
  "private": true,
  "description": "API project base on mrapi",
  "main": "index.js",
  "author": "mrapi",
  "license": "MIT",
  "scripts": {
    "start": "npm run generate && npm run migrate save -- --name=\"\" --create-db && npm run migrate up -- --create-db && npm run start:dev && npm run studio",
    "start:prod": "cross-env NODE_ENV=production node dist/app",
    "start:dev": "ts-node-dev --respawn --transpile-only ./src/app.ts",
    "start:debug": "ts-node-dev --inspect --respawn --transpile-only ./src/app.ts",
    "build": "tsc",
    "generate": "mrapi generate",
    "migrate": "mrapi migrate",
    "studio": "mrapi studio",
    "introspect": "mrapi introspect",
    "introspect:preview": "mrapi introspect --print",
    "db:seed": "../../node_modules/.pnpm/ts-node@8.10.2_typescript@3.9.5/node_modules/ts-node/dist/bin.js config/seed"
  },
  "dependencies": {
    "@mrapi/core": "workspace:*",
    "@prisma/client": "^2.2.2"
  },
  "devDependencies": {
    "@mrapi/cli": "workspace:*",
    "@prisma/cli": "^2.2.2",
    "cross-env": "^7.0.2",
    "ts-node-dev": "1.0.0-pre.51",
    "@mrapi/typegraphql-prisma": "0.6.0",
    "typescript": "^3.9.6"
  },
  "resolutions": {
    "graphql": "15.3.0"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
