{
  "name": "basic",
  "version": "0.0.1",
  "private": true,
  "description": "API project base on mrapi",
  "main": "index.js",
  "author": "mrapi",
  "license": "MIT",
  "scripts": {
    "start": "npm run gen && npm run db:save && npm run db:up && npm run start:dev",
    "start:prod": "cross-env NODE_ENV=production node dist/app",
    "start:dev": "ts-node-dev --respawn --transpileOnly ./src/app.ts",
    "start:debug": "ts-node-dev --inspect --respawn --transpileOnly ./src/app.ts",
    "build": "tsc",
    "gen": "cross-env verbose=true mrapi generate",
    "db:save": "mrapi db:save",
    "db:up": "mrapi db:up",
    "db:down": "mrapi db:down",
    "db:ui": "mrapi db:ui",
    "db:introspect": "mrapi db:introspect",
    "db:seed": "../../node_modules/.pnpm/ts-node@8.10.2_typescript@3.9.5/node_modules/ts-node/dist/bin.js config/seed"
  },
  "dependencies": {
    "@mrapi/core": "workspace:*",
    "@prisma/client": "^2.3.0"
  },
  "devDependencies": {
    "@mrapi/cli": "workspace:*",
    "@prisma/cli": "^2.3.0",
    "cross-env": "^7.0.2",
    "ts-node-dev": "1.0.0-pre.50",
    "typegraphql-prisma": "0.4.0",
    "typescript": "^3.9.7"
  },
  "resolutions": {
    "graphql": "15.3.0"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
