{
  "name": "dal-basic",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only ./src/app.ts",
    "prod": "cross-env NODE_ENV=production node dist/src/app",
    "build": "rm -rf dist && npm run generate && tsc",
    "clean": "rm -rf dist && rm -rf node_modules",
    "generate": "npm run generate:prisma1 && npm run generate:prisma2 && npm run generate:cnt1 && npm run generate:cnt2",
    "generate:prisma1": "npx prisma-multi-tenant generate --schema=\"./prisma/one.prisma\"",
    "generate:prisma2": "npx prisma-multi-tenant generate --schema=\"./prisma/two.prisma\"",
    "generate:cnt1": "npx cnt --schema=\"prisma/one.prisma\" --outDir=\"dist/types/one\" -s -mq -m -q -f -o --js",
    "generate:cnt2": "npx cnt --schema=\"prisma/two.prisma\" --outDir=\"dist/types/two\" -s -mq -m -q -f -o --js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mrapi/dal": "workspace:*",
    "cross-env": "^7.0.2",
    "prisma-multi-tenant": "2.3.1",
    "@prisma-multi-tenant/shared": "2.3.1",
    "@prisma/cli": "^2.4.1",
    "create-nexus-type": "1.2.5"
  },
  "devDependencies": {
    "typescript": "^3.9.6",
    "ts-node-dev": "1.0.0-pre.51"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
