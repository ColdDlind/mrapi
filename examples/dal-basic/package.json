{
  "name": "dal-basic",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only ./src/app.ts",
    "prod": "cross-env NODE_ENV=production node dist/src/app",
    "build": "rm -rf dist && npm run generate && tsc",
    "clean": "rm -rf dist && rm -rf node_modules",
    "fixbug": "node scripts/fixbugs.js",
    "generate": "npm run fixbug && npm run gen1 && npm run gen2",
    "gen1": "npx mrapi generate --name one",
    "gen2": "npx mrapi generate --name two"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mrapi/dal": "workspace:*",
    "@mrapi/cli": "workspace:*",
    "cross-env": "^7.0.2",
    "@prisma/cli": "^2.5.0",
    "prisma-multi-tenant": "2.3.1",
    "create-nexus-type": "1.2.5"
  },
  "devDependencies": {
    "typescript": "^3.9.6",
    "ts-node-dev": "1.0.0-pre.51"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
