{
  "name": "dal-basic",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only ./src/app.ts",
    "prod": "cross-env NODE_ENV=production node dist/src/app",
    "build": "rm -rf dist && npm run generate && tsc",
    "clean": "rm -rf dist && rm -rf node_modules",
    "generate": "npm run gen1 && npm run gen2",
    "gen1": "npx mrapi generate --name one",
    "gen2": "npx mrapi generate --name two",
    "gto": "node ./scripts/qto.js",
    "dmmf": "ts-node-dev --respawn --transpile-only ./scripts/dmmf.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mrapi/dal": "workspace:*",
    "@mrapi/cli": "workspace:*",
    "cross-env": "^7.0.2",
    "@prisma/cli": "^2.4.1",
    "prisma-multi-tenant": "2.3.1",
    "create-nexus-type": "1.2.6",
    "graphql-to-openapi": "1.5.0",
    "express-openapi": "7.0.1"
  },
  "devDependencies": {
    "fs-extra": "9.0.1",
    "prettier": "2.0.5",
    "typescript": "^4.0.2",
    "ts-node-dev": "1.0.0-pre.60"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
