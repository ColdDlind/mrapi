{
    "name": "example",
    "private": true,
    "version": "0.0.0",
    "main": "dist/main.ts",
    "scripts": {
      "clean": "rm -rf node_modules",
      "build": "yarn -s clean && yarn -s generate && tsc",
      "style": "prettier --write 'src/**/*'",
      "dev": "ts-node-dev --tree-kill --no-notify --respawn --transpile-only src/main.ts",
      "dev:migrate": "prisma2 migrate save --experimental -c && prisma2 migrate up --experimental -c",
      "db:save":"prisma migrate save --experimental",
      "db:up":"prisma migrate up --experimental",
      "generate": "yarn -s generate:prisma",
      "generate:prisma":"prisma generate",
      "db:seed": "../../node_modules/.pnpm/ts-node@8.10.2_typescript@3.9.7/node_modules/ts-node/dist/bin.js config/seed"
    },
    "dependencies": {
      "@mrapi/core": "workspace:*",
      "@prisma/client": "^2.2.2"
    },
    "devDependencies": {
      "@mrapi/cli": "workspace:*",
      "@prisma/cli": "^2.2.2",
      "@prisma/client": "^2.2.2",
      "cross-env": "^7.0.2",
      "ts-node-dev": "1.0.0-pre.51",
      "typescript": "^3.9.6"
    },
    "prettier": {
      "semi": false,
      "tabWidth": 2,
      "singleQuote": true,
      "trailingComma": "all"
    },
    "resolutions": {
      "graphql": "15.1.0"
    }
  }