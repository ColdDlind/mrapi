{
  "name": "mrapi-workspace",
  "version": "0.8.3",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "clean": "pnpm recursive run clean && rm -rf node_modules",
    "build": "pnpm run build --filter @mrapi/core && pnpm run build --filter @mrapi/cli && pnpm run build --filter create-mrapi-app",
    "watch": "pnpm run --parallel watch",
    "publish": "ts-node scripts/publish.ts",
    "publish:dry": "DRY_RUN=true ts-node scripts/publish.ts",
    "example:basic:generate": "pnpm run generate --filter ./examples/basic",
    "example:basic:dev": "pnpm run dev --filter ./examples/basic",
    "example:basic:db-save": "pnpm run db:save --filter ./examples/basic",
    "example:basic:db-up": "pnpm run db:up --filter ./examples/basic",
    "example:basic:db-seed": "pnpm run db:seed --filter ./examples/basic",
    "example:multi-tenant:generate": "pnpm run generate --filter ./examples/multi-tenant",
    "example:multi-tenant:dev": "pnpm run dev --filter ./examples/multi-tenant",
    "example:multi-tenant:db-save": "pnpm run db:save --filter ./examples/multi-tenant",
    "example:multi-tenant:db-up": "pnpm run db:up --filter ./examples/multi-tenant",
    "example:multi-tenant:db-seed": "pnpm run db:seed --filter ./examples/multi-tenant"
  },
  "devDependencies": {
    "chalk": "^4.1.0",
    "execa": "^4.0.3",
    "jest": "^26.1.0",
    "semver": "^7.3.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "resolutions": {
    "graphql": "15.3.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "__npminstall_done": false
}
