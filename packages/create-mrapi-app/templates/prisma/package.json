{
  "name": "mrapi-app",
  "version": "0.0.0",
  "main": "index.js",
  "author": "mrapi",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env DEBUG=prisma:query ts-node-dev --respawn --transpileOnly ./src/app.ts",
    "debug": "cross-env DEBUG=prisma:query ts-node-dev --inspect --respawn --transpileOnly ./src/app.ts",
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node dist/app",
    "generate": "mrapi generate",
    "db:save": "npx prisma migrate save --create-db --name '' --experimental",
    "db:up": "npx prisma migrate up --experimental",
    "db:down": "npx prisma migrate down --experimental",
    "db:ui": "npx prisma studio --experimental",
    "db:introspect": "npx prisma introspect",
    "db:seed": "ts-node config/seed"
  },
  "dependencies": {
    "@mrapi/core": "0.0.1-alpha.6"
  },
  "devDependencies": {
    "@mrapi/cli": "0.0.1-alpha.6",
    "cross-env": "^7.0.2",
    "typescript": "^3.8.3",
    "ts-node-dev": "^1.0.0-pre.44"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
