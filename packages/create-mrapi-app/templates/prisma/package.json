{
  "name": "mrapi-app",
  "main": "index.js",
  "author": "mrapi",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env DEBUG=prisma:query ts-node-dev --respawn --transpileOnly ./src/app.ts",
    "debug": "cross-env DEBUG=prisma:query ts-node-dev --inspect --respawn --transpileOnly ./src/app.ts",
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node dist/app",
    "generate": "mrapi generate",
    "db": "npm run db:save && npm run db:up",
    "db:save": "mrapi db:save",
    "db:up": "mrapi db:up",
    "db:ui": "mrapi db:ui"
  },
  "dependencies": {
    "@mrapi/core": "0.0.1-alpha.3"
  },
  "devDependencies": {
    "@mrapi/cli": "0.0.1-alpha.3",
    "cross-env": "^7.0.2",
    "typescript": "^3.8.3",
    "ts-node-dev": "^1.0.0-pre.44"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
